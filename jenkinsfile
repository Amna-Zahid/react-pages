pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          bat 'docker pull node:alpine'
                     }
                }
                stage("library") {
                     steps {
                          bat 'npm install'
                     }
                }
                stage("build") {
                     steps {
                          bat 'npm run build'
                     }
                }
                stage("test") {
                     steps {
                          powershell 'build.ps1'
                     }
                }
           }
      }
